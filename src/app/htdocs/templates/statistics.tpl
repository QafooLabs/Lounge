<h2>Tweet statistics</h2>

<dl>
{{#statistics}}
    <dt>{{date}}</dt>
    <dd>{{count}}</dd>
{{/statistics}}
</dl>

<p>
    Group by:
    <a href="/statistics?groupLevel=1">Year</a>
    <a href="/statistics?groupLevel=2">Month</a>
    <a href="/statistics?groupLevel=3">Day</a>
    <a href="/statistics?groupLevel=4">Hour</a>
    <a href="/statistics?groupLevel=5">Minute</a>
    <a href="/statistics?groupLevel=6">Second</a>
</p>
